{"ast":null,"code":"export default {\n  name: 'employee-table',\n  props: {\n    employees: Array\n  },\n  data() {\n    return {\n      editing: null,\n      employee: {\n        name: '',\n        email: ''\n      },\n      cachedEmployee: null\n    };\n  },\n  methods: {\n    editMode(employee) {\n      this.editing = employee.id;\n      this.cachedEmployee = Object.assign({}, employee);\n    },\n    editEmployee(employee) {\n      if (employee.name == '' || employee.email == '') {\n        return;\n      }\n      this.$emit('edit:employee', employee.id, employee);\n      this.editing = null;\n    }\n  }\n};","map":{"version":3,"names":["name","props","employees","Array","data","editing","employee","email","cachedEmployee","methods","editMode","id","Object","assign","editEmployee","$emit"],"sources":["C:\\Users\\hushanh\\Thushanth_Vue_Example\\Vue\\myapp\\src\\components\\EmployeeTable.vue"],"sourcesContent":["<template>\r\n    <div id=\"employee-table\">\r\n        <p v-if=\"employees.length<1\" class=\"empty-table\">No Employees</p>\r\n        <table v-else class=\"table-container\">\r\n            <thead>\r\n                <tr>\r\n                    <th>Emplyee name</th>\r\n                    <th>Emplyee email</th>\r\n                    <th>Action</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"employee in employees\" :key=\"employee.id\">\r\n                    <td v-if=\"editing==employee.id\"><input type =\"text\" v-model=\"employee.name\"></td>\r\n                    <td v-else>{{ employee.name }}</td>\r\n                    <td v-if=\"editing==employee.id\"><input type =\"text\" v-model=\"employee.email\"></td>\r\n                    <td v-else>{{ employee.email }}</td>\r\n                    <td v-if=\"editing==employee.id\">\r\n                        <button @click=\"editEmployee(employee)\">Save</button>\r\n                        <button @click=\"cancelEdit(employee)\">Cancel</button>\r\n                    </td>\r\n                    <td v-else>\r\n                        <button @click=\"editMode(employee)\">Edit</button>\r\n                        <button @click=\"$emit('delete:employee',employee.id)\">Delete</button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    name:'employee-table',\r\n    props:{\r\n        employees: Array\r\n    },\r\n    data(){\r\n        return{\r\n            editing:null,\r\n            employee:{\r\n                name:'',\r\n                email:''\r\n            },\r\n            cachedEmployee:null\r\n        }\r\n    },\r\n    methods:{\r\n        editMode(employee){\r\n            this.editing = employee.id;\r\n            this.cachedEmployee = Object.assign({},employee);\r\n        },\r\n        editEmployee(employee){\r\n            if(employee.name=='' || employee.email== ''){\r\n                return;\r\n            }\r\n            this.$emit('edit:employee',employee.id,employee)\r\n            this.editing=null;\r\n        },\r\n        \r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\nbutton{\r\n    margin: 0 0.5rem 0 0;   \r\n}\r\n</style>"],"mappings":"AA+BA,eAAe;EACXA,IAAI,EAAC,gBAAgB;EACrBC,KAAK,EAAC;IACFC,SAAS,EAAEC;EACf,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,OAAO,EAAC,IAAI;MACZC,QAAQ,EAAC;QACLN,IAAI,EAAC,EAAE;QACPO,KAAK,EAAC;MACV,CAAC;MACDC,cAAc,EAAC;IACnB;EACJ,CAAC;EACDC,OAAO,EAAC;IACJC,QAAQA,CAACJ,QAAQ,EAAC;MACd,IAAI,CAACD,OAAM,GAAIC,QAAQ,CAACK,EAAE;MAC1B,IAAI,CAACH,cAAa,GAAII,MAAM,CAACC,MAAM,CAAC,CAAC,CAAC,EAACP,QAAQ,CAAC;IACpD,CAAC;IACDQ,YAAYA,CAACR,QAAQ,EAAC;MAClB,IAAGA,QAAQ,CAACN,IAAI,IAAE,EAAC,IAAKM,QAAQ,CAACC,KAAK,IAAG,EAAE,EAAC;QACxC;MACJ;MACA,IAAI,CAACQ,KAAK,CAAC,eAAe,EAACT,QAAQ,CAACK,EAAE,EAACL,QAAQ;MAC/C,IAAI,CAACD,OAAO,GAAC,IAAI;IACrB;EAEJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}